<p><%= link_to 'New Article', new_article_path %></p>
<h1>Edit Article</h1>

<%= form_for(@article) do |form| %>

  <p>
  <%= form.label :title %>
  <%= form.text_field :title %>
  </p>

  <p>
  <%= form.label :content %>
  <%= form.text_field :content %>
  </p>

  <p>
  <%= form.submit %>
  </p>

<% end %>
